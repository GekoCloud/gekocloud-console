{
  "name": "@pitrix/lego-ui",
  "version": "0.1.6-13",
  "description": "QingCloud UI Component",
  "keywords": [
    "components",
    "css",
    "flexbox",
    "framework",
    "lego-ui",
    "qingcloud",
    "react",
    "responsive",
    "sass"
  ],
  "repository": "git@git.internal.yunify.com:elvis/lego-ui.git",
  "license": "MIT",
  "author": "QingCloud",
  "files": ["lib"],
  "main": "lib/components",
  "style": "lib/scss",
  "scripts": {
    "build": "node build/build-site",
    "build:badge": "node build/build-badge",
    "build:icon": "npm run svgo && node build/build-icon",
    "build:lib": "rm -rf ./lib && BABEL_ENV=publish babel src -D -d lib",
    "build:sass": "node build/build-sass",
    "dev": "node build/dev-server",
    "eslint": "eslint '{site,src}/**/*.{js,jsx,md}' --fix",
    "formatter": "prettier-eslint 'site/**/*.md' --write",
    "lint": "npm run eslint && npm run stylelint",
    "prepublishOnly": "npm run build:lib && npm run build:sass && npm run build:badge",
    "release": "npm run prepublishOnly && standard-version",
    "stylelint": "stylelint '{site,src}/**/*.scss' --syntax=scss --fix",
    "svgo": "svgo --enable=sortAttrs --disable=removeViewBox --multipass --pretty --indent=2 -q -r -f ./src/assets/icons/",
    "cspell": "cspell '{src,site}/**/*.{js,jsx,scss}'",
    "test": "NODE_ENV=test jest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
